{
  "name": "Hemul",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Lägger till områdesinformation till annonser på en bostadssajt.",
  "permissions": [
    "https://hemul.fria.nu/ajaxhandler.php", "http://hemul.fria.nu/ajaxhandler.php"
  ],
  "icons": {
    "48" : "sample-48.png",
    "128" : "sample-128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://www.hemnet.se/bostad/*", "https://www.hemnet.se/bostad/*"],
      "js" : 	["jquery-2.1.1.min.js", "contentscript.js", "angular.1.3.14.min.js", "hemul.js"]
    }
  ],
  "web_accessible_resources": ["angular.min.js.map"]
}
